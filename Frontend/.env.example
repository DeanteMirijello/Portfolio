# Public env vars available in client-side code must start with PUBLIC_
# API base used by pages at runtime
PUBLIC_API_BASE="http://localhost:3001"

# Auth0 SPA settings (used by nav/auth scripts)
PUBLIC_AUTH0_DOMAIN=your-tenant.us.auth0.com
PUBLIC_AUTH0_CLIENT_ID=your-spa-client-id
PUBLIC_AUTH0_AUDIENCE=your-api-identifier
# If you mapped roles into a custom claim, keep this value in sync
PUBLIC_AUTH0_ROLES_CLAIM=https://your.app/roles
# Optional shortcut: emails treated as admin on the client
PUBLIC_ADMIN_EMAILS=
